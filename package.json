{
  "name": "1fox-content-service",
  "version": "1.0.0",
  "private": true,
  "description": "1Fox Content Service",
  "author": "1Fox Team <dev.1foxglobal@gmail.com>",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "type-check": "tsc --pretty --noEmit --skipLibCheck",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "prepare": "husky install",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm migration:generate ./src/migrations/migration -d ./src/config/dbConfig.ts -p",
    "migration:run": "yarn typeorm migration:run -d ./src/config/dbConfig.ts",
    "migration:revert": " yarn typeorm migration:revert -d ./src/config/dbConfig.ts",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "eslint --fix .",
      "git add --force"
    ],
    "./**/*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.121.0",
    "@aws-sdk/signature-v4-crt": "^3.118.0",
    "@types/uuid": "^8.3.4",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "bullmq": "^1.86.5",
    "celebrate": "^15.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.3",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "helmet": "^5.1.0",
    "http-status": "^1.5.2",
    "ioredis": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.6.0",
    "mailgun.js": "^7.0.4",
    "method-override": "^3.0.0",
    "mongodb": "^4.8.1",
    "ms": "^2.1.3",
    "multer": "^1.4.4",
    "nanoid": "^4.0.0",
    "pg": "^8.7.3",
    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.4.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.7",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@faker-js/faker": "^7.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/method-override": "^0.0.32",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.3",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.0"
}
