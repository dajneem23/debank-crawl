{
  "name": "sv-backend",
  "version": "1.0.1",
  "private": true,
  "description": "ShareViet.vn Backend",
  "author": "Mobytelab <development@mobytelab.com>",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "type-check": "tsc --pretty --noEmit --skipLibCheck",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "eslint --fix .",
      "git add --force"
    ],
    "./**/*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.53.1",
    "@aws-sdk/signature-v4-crt": "^3.53.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bullmq": "^1.64.0",
    "celebrate": "^15.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "ioredis": "^4.28.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "mailgun.js": "^4.1.4",
    "method-override": "^3.0.0",
    "mongodb": "^4.2.1",
    "ms": "^2.1.3",
    "multer": "^1.4.4",
    "nanoid": "^3.1.30",
    "redis": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.7",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.178",
    "@types/method-override": "^0.0.32",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  }
}
