{
  "_id": "company",
  "startUrl": ["https://cryptoslate.com/companies/"],
  "selectors": [
    {
      "delay": 0,
      "id": "categories_link",
      "multiple": true,
      "parentSelectors": ["_root"],
      "selector": "a.load-more",
      "type": "SelectorLink"
    },
    {
      "id": "page",
      "paginationType": "clickMore",
      "parentSelectors": ["categories_link", "page"],
      "selector": ".site-navigation a:contains('Next page â†’')",
      "type": "SelectorPagination"
    },
    {
      "delay": 0,
      "id": "page-detail",
      "multiple": true,
      "parentSelectors": ["page"],
      "selector": ".company a",
      "type": "SelectorLink"
    },
    {
      "delay": 0,
      "id": "name",
      "multiple": false,
      "parentSelectors": ["page-detail"],
      "regex": "",
      "selector": ".name-info h1",
      "type": "SelectorText"
    },
    {
      "delay": 0,
      "id": "short-description",
      "multiple": false,
      "parentSelectors": ["page-detail"],
      "regex": "",
      "selector": ".name-info > .meta",
      "type": "SelectorText"
    },
    {
      "delay": 0,
      "id": "about ",
      "multiple": false,
      "parentSelectors": ["page-detail"],
      "regex": "",
      "selector": "#company-about > .post-box   p",
      "type": "SelectorText"
    },
    {
      "delay": 0,
      "extractAttribute": "href",
      "id": "twitter",
      "parentSelectors": ["page-detail"],
      "selector": ".social-links a.twitter",
      "type": "SelectorGroup"
    },
    {
      "delay": 0,
      "extractAttribute": "href",
