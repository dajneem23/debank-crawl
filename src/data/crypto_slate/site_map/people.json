{
  "_id": "people",
  "startUrl": ["https://cryptoslate.com/people/"],
  "selectors": [
    {
      "id": "categories",
      "parentSelectors": ["_root"],
      "type": "SelectorLink",
      "selector": "a.load-more",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "page",
      "parentSelectors": ["categories", "page"],
      "paginationType": "clickMore",
      "selector": ".site-navigation a:contains('Next page â†’')",
      "type": "SelectorPagination"
    },
    {
      "id": "detail",
      "parentSelectors": ["page"],
      "type": "SelectorLink",
      "selector": " .object-post a",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "about",
      "parentSelectors": ["detail"],
      "type": "SelectorText",
      "selector": "section#who-is",
      "multiple": false,
      "delay": 0,
      "regex": ""
    },
    {
      "id": "current_works",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#current-work li div , #current-work a",
      "delay": 0,
      "extractAttribute": "title"
    },
    {
      "id": "current_work_names",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#current-work span.name",
      "delay": 0,
      "extractAttribute": ""
    },
    {
      "id": "educations",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#education li",
      "delay": 0,
      "extractAttribute": ""
    },
    {
      "id": "previous_works",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#previous-work a, #previous-work div.content ",
      "delay": 0,
      "extractAttribute": "title"
    },
    {
      "id": "previous_work_names",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#previous-work span.name",
      "delay": 0,
      "extractAttribute": ""
    },
    {
      "id": "twitter",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "a.twitter",
      "delay": 0,
      "extractAttribute": "href"
    },
    {
      "id": "linkedin",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "a.linkedin",
      "delay": 0,
      "extractAttribute": "href"
    },
    {
      "id": "instagram",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "a.instagram",
      "delay": 0,
      "extractAttribute": "href"
    },
    {
      "id": "github",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "a.github",
      "delay": 0,
      "extractAttribute": "href"
    },
    {
      "id": "facebook",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "a.facebook",
      "delay": 0,
      "extractAttribute": "href"
    },
    {
      "id": "tags",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": ".breadcrumbs a:nth-of-type(n+3)",
      "delay": 0,
      "extractAttribute": ""
    },
    {
      "id": "website",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": ".cta-buttons a",
      "delay": 0,
      "extractAttribute": "href"
    },
    {
      "id": "previous_works_date",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#previous-work a span.years-active, .content span.years-active",
      "delay": 0,
      "extractAttribute": ""
    },
    {
      "id": "previous_works_position",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#previous-work a span.role, #previous-work .content span.role",
      "delay": 0,
      "extractAttribute": ""
    },
    {
      "id": "current_works_position",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#current-work.post a span.role, #current-work.post .content span.role",
      "delay": 0,
      "extractAttribute": ""
    },
    {
      "id": "position",
      "parentSelectors": ["detail"],
      "type": "SelectorText",
      "selector": ".name-info span.meta",
      "multiple": false,
      "delay": 0,
      "regex": ""
    },
    {
      "id": "name",
      "parentSelectors": ["detail"],
      "type": "SelectorText",
      "selector": ".name-info h1",
      "multiple": false,
      "delay": 0,
      "regex": ""
    },
    {
      "id": "verified",
      "parentSelectors": ["detail"],
      "type": "SelectorText",
      "selector": "span.small",
      "multiple": false,
      "delay": 0,
      "regex": ""
    },
    {
      "id": "youtube",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": ".social-links a.youtube",
      "delay": 0,
      "extractAttribute": "href"
    },
    {
      "id": "avatar",
      "parentSelectors": ["detail"],
      "type": "SelectorImage",
      "selector": "#profile-header img.logo",
      "multiple": false,
      "delay": 0
    },
    {
      "id": "short-description",
      "parentSelectors": ["detail"],
      "type": "SelectorText",
      "selector": ".name-info > .meta",
      "multiple": false,
      "delay": 0,
      "regex": ""
    }
  ]
}
