{
  "_id": "product",
  "startUrl": ["https://cryptoslate.com/products/"],
  "selectors": [
    {
      "id": "categories",
      "parentSelectors": ["_root"],
      "type": "SelectorLink",
      "selector": ".load-more ",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "page",
      "parentSelectors": ["categories", "page"],
      "paginationType": "clickMore",
      "selector": ".site-navigation a:contains('Next page â†’')",
      "type": "SelectorPagination"
    },
    {
      "id": "detail",
      "parentSelectors": ["page"],
      "type": "SelectorLink",
      "selector": ".product a.clearfix",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "name",
      "parentSelectors": ["detail"],
      "type": "SelectorText",
      "selector": ".name-info h1",
      "multiple": false,
      "delay": 0,
      "regex": ""
    },
    {
      "id": "tags",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": ".meta > a",
      "delay": 0,
      "extractAttribute": ""
    },
    {
      "id": "website",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": ".sidebar .cta-buttons a",
      "delay": 0,
      "extractAttribute": "href"
    },
    {
      "id": "about",
      "parentSelectors": ["detail"],
      "type": "SelectorText",
      "selector": "#product-about article p",
      "multiple": true,
      "delay": 0,
      "regex": ""
    },
    {
      "id": "gallery",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#product-gallery img",
      "delay": 0,
      "extractAttribute": "src"
    },
    {
      "id": "information",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "#dapp-information a[title], span.item",
      "delay": 0,
      "extractAttribute": "title"
    },
    {
      "id": "contract_addresses",
      "parentSelectors": ["detail"],
      "type": "SelectorGroup",
      "selector": "a.contract-address",
      "delay": 0,
      "extractAttribute": "href"
    },
    {
      "id": "features",
      "parentSelectors": ["detail"],
